POS_mixed_UD1_v2 {
    Algorithm "libnlp::algo::PosTagger"

    Parameters {
        ModelFile "libnlp:models/POS_mixed_UD1_v2.bin"
        AutoTokenType true
    }

}

DepParse_news_UD1.3_v4 {
